{
  "name": "admob-plus-cordova-native",
  "version": "0.2.7",
  "description": "Cordova Plugin for AdMob Plus Native Ad",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "version": "yarn sync-pkg-version --cordova-plugin",
    "build": "tsc -b && rollup -c",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "admob-plus-cordova": "^1.20.1"
  },
  "devDependencies": {
    "@admob-plus-internal/rollup-config": "file:../../internal/rollup-config",
    "@types/cordova": "^0.0.34",
    "@types/plist": "^3.0.2",
    "cordova": "10.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.56.0",
    "sync-package-version": "^0.1.0",
    "typescript": "^4.3.5"
  },
  "author": "Ratson",
  "license": "MIT",
  "repository": "admob-plus/admob-plus",
  "homepage": "https://admob-plus.github.io/docs/cordova-native",
  "files": [
    "*.ts",
    "*.md",
    "*.xml",
    "lib",
    "scripts",
    "src",
    "ts",
    "www",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "cordova",
    "admob",
    "plugin",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "monetization",
    "typescript"
  ],
  "cordova": {
    "id": "admob-plus-cordova-native",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "eslintConfig": {
    "extends": [
      "concise-esnext"
    ]
  }
}
