{
  "name": "admob-plus-cordova",
  "version": "1.27.0",
  "description": "Trustable Google AdMob Cordova Plugin",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "version": "yarn cordova-plus plugin version",
    "build": "wireit",
    "build:lib": "wireit",
    "build:scripts": "wireit",
    "test": "cordova-plus plugin test"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "plist": "^3.0.5",
    "replace-in-file": "^6.3.5"
  },
  "devDependencies": {
    "@admob-plus-internal/rollup-config": "file:../../internal/rollup-config",
    "@tsconfig/node10": "^1.0.9",
    "@types/cordova": "^0.0.34",
    "@types/plist": "^3.0.2",
    "cordova": "11.0.0",
    "cordova-plus": "^0.4.0",
    "cordova-ts-hook": "^0.0.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.75.6",
    "typescript": "~4.7.3",
    "wireit": "^0.5.0"
  },
  "author": "Ratson",
  "license": "MIT",
  "repository": "admob-plus/admob-plus",
  "homepage": "https://admob-plus.github.io/docs/cordova",
  "funding": "https://admob-plus.github.io/funding",
  "files": [
    "*.ts",
    "*.md",
    "*.xml",
    "lib",
    "scripts",
    "src",
    "ts",
    "www",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "cordova",
    "admob",
    "plugin",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "monetization",
    "typescript"
  ],
  "cordova": {
    "id": "admob-plus-cordova",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "wireit": {
    "build:lib": {
      "command": "cordova-plus plugin build --input=./src/www/admob.ts --lib"
    },
    "build:scripts": {
      "command": "tsc -p scripts"
    },
    "build": {
      "dependencies": [
        "build:lib",
        "build:scripts"
      ]
    }
  }
}
