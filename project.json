{
  "projectType": "application",
  "root": ".",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b --verbose"
      },
      "dependsOn": [
        "build-cordova"
      ]
    },
    "build-cordova": {
      "executor": "nx:run-commands",
      "options": {
      "command": "nx build admob-plus-cordova"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "lerna run test"
      },
      "dependsOn": [
        "jest"
      ]
    },
    "jest": {
      "executor": "@nrwl/jest:jest",
      "outputs": [],
      "options": {
        "jestConfig": "jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
