{
  "name": "example.admob.plus.rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "react-native clean-project-auto --clean-android-project --keep-node_modules",
    "upgrade": "git init && git add . && git commit -m 'upgrading' && react-native upgrade; del .git"
  },
  "dependencies": {
    "@admob-plus/react-native": "*",
    "react": "17.0.2",
    "react-native": "0.68.2"
  },
  "devDependencies": {
    "@admob-plus-internal/cli": "file:../../internal/cli",
    "@babel/core": "^7.17.12",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/jest": "^27.5.1",
    "@types/react-native": "^0.65.1",
    "@types/react-test-renderer": "^17.0.1",
    "admob-plus": "file:../../packages/cli",
    "babel-jest": "^27.2.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "pod-install": "^0.1.27",
    "react-native-clean-project": "^3.6.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.6.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
